{
  "name": "robonomics-ui-guide",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "export NODE_OPTIONS=--openssl-legacy-provider && npx kill-port 8080 && vue-cli-service serve --port 8080",
    "serve-newport": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "build-mine": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@polkadot/ui-keyring": "^3.6.4",
    "core-js": "^3.6.5",
    "robonomics-ui-vue": "^0.3.0",
    "simple-syntax-highlighter": "^2.2.0",
    "vue": "^3.0.0",
    "vue-router": "^4.2.5",
    "vue-template-compiler": "^2.6.14",
    "vue3-highlightjs": "^1.0.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "url": "^0.11.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
